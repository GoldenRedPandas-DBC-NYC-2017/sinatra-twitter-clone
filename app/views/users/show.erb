<h1> Current User </h1>

<!-- Investigate, has to be a better way to put this in scope of the html page -->
<% current_user %>
  <% if logged_in? %>
    <h3> <%= @current_user.username %> </h3><br><br>
    <h3> <%= @current_user.email %> </h3><br><br>
    <h3> <%= @current_user.created_at %> </h3><br><br>
  <% end %>

<a href="/">Home</a><br>
<a href="/sessions/logout"> Log Out</a>
